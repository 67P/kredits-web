  "scripts": {
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "test": "ember test",
    "start": "KREDITS_APM_DOMAIN=open.aragonpm.eth ember serve",
    "start:local": "WEB3_PROVIDER_URL=http://localhost:7545 ember s",
