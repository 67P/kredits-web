{{#each proposals as |proposal|}}
  {{proposal-list/item
    proposal=proposal
    selectedProposals=selectedProposals
    toggleSelect=(action 'toggleSelect')
  }}
{{/each}}

{{#if confirmProposals}}
  <center>
    <button
      disabled={{submitButtonDisabled}}
      {{action confirmProposals selectedProposals}}
    >
      Submit selected
    </button>
  </center>
{{/if}}
